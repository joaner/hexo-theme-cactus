<div id="header-post">
  <span id="menu">
    <span id="nav">
      <ul>
        <% for (var i in theme.nav) { %>
          <li><a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a></li>
        <% } %>
      </ul>
    </span>
    <br/>
    <div id="toc">
      <%- toc(page.content) %>
    </div>
  </span>
</div>
